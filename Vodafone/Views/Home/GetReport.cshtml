<link rel="stylesheet" href="~/Content/GetReport.css" />
@model  Vodafone.Models.fnFormulaVPC_Result
@{
    ViewData["Title"] = "";
}
<a href="@Url.Action("ReportPDF", "Home")" class="button bacground-grey"><h4>Export to PDF</h4></a>
<section class="contentblock">
    <div class="container">
            <div class="col-sm-12 pb0">
                <div class="heading1">
                    <h3><span><b>VPC ELECTRICITY POLICY</b></span></h3>
                    <div class="line2"></div>
                </div>
            </div>
            <div class="col-sm-12">
                    <h3 class="color-grey"><span><b>APPENDIX A</b></span></h3>
            </div>
            <div class="row col-xs-12 pb0">
                <div class="col-sm-12 col-md-6 col-lg-6 col-xl-9 fb">
                <h4 class="bacground-grey"><span><b>OPERATING COMPANY</b></span></h4>
                </div>
                <div class="col-sm-12 col-md-6 col-lg-6 col-xl-3"><h4 class="bacground-orange"><span>@ViewBag.raportname</span></h4></div>
                <div class="col-sm-7 col-md-6 col-lg-10 col-xl-9">
                    <h5 class="yelou-greed"><span><b>REPORTING PERIOD:</b></span></h5>
                </div>
                <div class="col-sm-5 col-md-6 col-lg-2 col-xl-3">
                    <div class="col-sm-12 wbc2"><b>@DateTime.Now.ToShortDateString()</b></div>
                </div>
            </div>
            <div class="col-sm-12 pb0">
                <div class="row col-xs-12 col-lg-10 col-xl-9 pb0">
                    <div class="col-sm-12 wbc1"></div>
                    <div class="col-md-12 col-lg-7 pb0">
                        <div class="col-sm-12 bbc">High electricity prices using historical volatility</div>
                    </div>
                    <div class="col-md-12 col-lg-5 pb0">
                        <div class="col-sm-12 ybc"><b>@ViewBag.High.ToString("0.##") @ViewBag.CurrrencyCode/@ViewBag.UnitName</b></div>
                    </div>
                    <div class="col-sm-12 wbc1 clear"></div>
                    <div class="col-md-12 col-lg-7 pb0">
                        <div class="col-sm-12 bbc">Medium electricity prices using historical volatility</div>
                    </div>
                    <div class="col-md-12 col-lg-5 pb0">
                        <div class="col-sm-12 ybc"><b>@ViewBag.Medium.ToString("0.##")</b>-<b>@ViewBag.High.ToString("0.##") @ViewBag.CurrrencyCode/@ViewBag.UnitName</b></div>
                    </div>
                    <div class="col-sm-12 wbc1 clear"></div>
                    <div class="col-md-12 col-lg-7 pb0">
                        <div class="col-sm-12 bbc">Low electricity prices using historical volatility</div>
                    </div>
                    <div class="col-md-12 col-lg-5 pb0">
                        <div class="col-sm-12 ybc"><b>@ViewBag.Low.ToString("0.##")</b>-<b>@ViewBag.Medium.ToString("0.##") @ViewBag.CurrrencyCode/@ViewBag.UnitName</b></div>
                    </div>
                    <div class="col-sm-12 wbc1 clear"></div>
                    <div class="col-md-12 col-lg-7 pb0">
                        <div class="col-sm-12 bbc">Ultra-low electricity prices using historical volatility</div>
                    </div>
                    <div class="col-md-12 col-lg-5 pb0">
                        <div class="col-sm-12 ybc"><b>@ViewBag.Low.ToString("0.##") @ViewBag.CurrrencyCode/@ViewBag.UnitName</b></div>
                    </div>
                    <div class="col-sm-12 wbc1 clear"></div>
                    <div class="col-md-12 col-lg-7 pb0">
                        <div class="col-sm-12 bbc">Lowest price during reporting period</div>
                    </div>
                    <div class="col-md-12 col-lg-5 pb0">
                        <div class="col-sm-12 ybc"><b>@ViewBag.MinMarket_Value.ToString("0.##") @ViewBag.CurrrencyCode/@ViewBag.UnitName</b><b> on @ViewBag.MinMarket_Date.ToShortDateString()</b></div>
                    </div>
                    <div class="col-sm-12 wbc1 clear"></div>
                    <div class="bbc col-md-12 col-lg-4 pb0">
                        <div class="col-sm-12 bbc1">Prices were (tick one box)</div>
                    </div>
                    <div class="col-sm-12 col-md-6 col-lg-2 pb0">
                        <div class="wbc">
                            <div class="col-xs-12 col-sm-12 pb0">High</div>
                        </div>
                        <div class="ybc1">
                            <div class="col-xs-12 col-sm-12 pb0">@ViewBag.Highx</div>
                        </div>
                    </div>
                    <div class="col-sm-12 col-md-6 col-lg-2 pb0">
                        <div class="wbc">
                            <div class="col-xs-12 col-sm-12 pb0">Medium</div>
                        </div>
                        <div class="ybc1">
                            <div class="col-xs-12 col-sm-12 pb0">@ViewBag.Mediumx</div>
                        </div>
                    </div>
                    <div class="col-sm-12 col-md-6 col-lg-2 pb0">
                        <div class="wbc">
                            <div class="col-xs-12 col-sm-12 pb0">Low</div>
                        </div>
                        <div class="ybc1">

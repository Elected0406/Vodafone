            <td colspan="4" style="text-align: center;">
                <div><b></b></div>
            </td>
            <td colspan="4" style="text-align: center; border: 1px solid rgb(191,191,191);">
                <div style="height: 20px;">@ViewBag.MaxPriceFixed.ToString("0.##") @ViewBag.CurrrencyCode/@ViewBag.UnitName</div>

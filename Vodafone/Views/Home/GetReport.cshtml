        {
            if (ViewBag.Year2 != null)
            {
                if (ViewBag.PolicyCompliant2 != null)
                {
                    if (ViewBag.CurrrencyCode != null)
                    {
                        if (ViewBag.UnitName != null)
                        {
                            <tbody>
                                <tr>
                                    <td style="height: 20px;"></td>
                                </tr>
                                <tr>
                                    <td colspan="3">
                                        <h4 style="text-align: left; padding-bottom: 10px; border-bottom: 2px solid rgb(237,125,49);"><b style="color: rgb(89,89,89);">Fixed in @ViewBag.Year2</b></h4>

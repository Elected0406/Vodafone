                            <div class="col-sm-12 col-md-6 col-lg-2 pb0 fd">
                                <div class="col-sm-12 fd"><b></b></div>
                                <div class="col-sm-12 fd"><b></b></div>
                            </div>
                            <div class="col-sm-12 col-md-6 col-lg-2 pb0">
                                <div class="col-sm-12 wbc">Price fixed</div>
                                <div class="col-sm-12 ybc"><b>@ViewBag.MaxPriceFixed2.ToString("0.##") @ViewBag.CurrrencyCode/@ViewBag.UnitName</b></div>
                            </div>
                            <div class="col-sm-12 col-md-6 col-lg-2 fd pb0">
                                <div class="col-sm-12 fd"><b></b></div>
                                <div class="col-sm-12 fd"><b></b></div>
                            </div>
                            <div class="col-sm-12 col-md-6 col-lg-2 pb0">
                                <div class="col-sm-12 wbc">MW</div>
                                <div class="col-sm-12 ybc"><b>@ViewBag.VolumeFixProcent2.ToString("0.##")% of FC</b></div>
                            </div>
                            <div class="ccol-sm-12 col-md-6 col-lg-2 pb0">
                                <div class="col-sm-12 wbc">Policy compliant</div>
                                <div class="col-sm-12 ybc"><b>@ViewBag.PolicyCompliant3</b></div>
                            </div>
                        </div>
                        <div class="col-sm-4"></div>
                    </div>
                </div>

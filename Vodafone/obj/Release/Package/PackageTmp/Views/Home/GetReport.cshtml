<link rel="stylesheet" href="~/Content/GetReport.css"/>
@model  Vodafone.Models.fnFormulaVPC_Result
@{
    ViewData["Title"] = "";
}
<table width="100%">
    <tbody>
        <tr>
            <td colspan="9">
                <h3 style="text-align: left;padding-bottom: 10px; border-bottom: 2px solid rgb(237,125,49);"><b style="color: rgb(89,89,89);">VPC ELECTRICITY POLICY</b></h3>
            </td>
            <td colspan="15">
                <div style="text-align: left;padding-bottom: 33px;  border-bottom: 2px solid rgb(191,191,191);"></div>
            </td>
        </tr>
        <tr>
            <td colspan="24">
                <h3 style="text-align: left;"><b style="color: rgb(89,89,89);">APPENDIX A</b></h3>
            </td>
        </tr>
        <tr>
            <td colspan="18">
                <h5 style="text-align: left;"><b style="color: rgb(89,89,89);">OPERATING COMPANY</b></h5>
            </td>
            <td colspan="1"></td>
            <td colspan="5">
                <h5 style="text-align: center; padding: 10px 0px 10px 0px; background-color:rgb(237,125,49)"><b style="color: rgb(89,89,89);">@ViewBag.raportname</b></h5>
            </td>
        </tr>
        <tr>
            <td colspan="18">
                <h6 style="text-align: right; color:rgb(237,125,49)"><b>REPORTING PERIOD:</b></h6>
            </td>
            <td colspan="1"></td>
            <td colspan="5">
                <h6 style="text-align: center; border: 1px solid rgb(191,191,191);"><b style="color: rgb(89,89,89);">@DateTime.Now.ToShortDateString()</b></h6>
            </td>
        </tr>
        <tr>
            <td style="height: 20px;"></td>
        </tr>
        <tr>
            <td colspan="12" style="border: 1px solid rgb(191,191,191);">High electricity prices using historical volatility</td>
            <td colspan="6" style="text-align: center; border: 1px solid rgb(191,191,191);"><b style="color: rgb(89,89,89);">@ViewBag.High.ToString("0.##") @ViewBag.CurrrencyCode/@ViewBag.UnitName</b></td>
            <td colspan="6"></td>
        </tr>
        <tr>
            <td style="height: 20px;"></td>
        </tr>
        <tr>
            <td colspan="12" style="border: 1px solid rgb(191,191,191);"><div>Medium electricity prices using historical volatility</div></td>
            <td colspan="6" style="text-align: center; border: 1px solid rgb(191,191,191);"><div><b style="color: rgb(89,89,89);">@ViewBag.Medium.ToString("0.##")- @ViewBag.High.ToString("0.##") @ViewBag.CurrrencyCode/@ViewBag.UnitName</div></b></td>
            <td colspan="6"></td>
        </tr>
        <tr>
            <td style="height: 20px;"></td>
        </tr>
        <tr>
            <td colspan="12" style="border: 1px solid rgb(191,191,191);">Low electricity prices using historical volatility</td>
            <td colspan="6" style="text-align: center; border: 1px solid rgb(191,191,191);"><b style="color: rgb(89,89,89);">@ViewBag.Low.ToString("0.##")-@ViewBag.Medium.ToString("0.##") @ViewBag.CurrrencyCode/@ViewBag.UnitName</b></td>
            <td colspan="6"></td>
        </tr>
        <tr style="height: 20px;"></tr>
        <tr>
            <td colspan="12" style="border: 1px solid rgb(191,191,191);">Ultra-low electricity prices using historical volatility</td>
            <td colspan="6" style="text-align: center; border: 1px solid rgb(191,191,191);"><b style="color: rgb(89,89,89);">@ViewBag.Low.ToString("0.##") @ViewBag.CurrrencyCode/@ViewBag.UnitName</b></td>
            <td colspan="6"></td>
        </tr>
        <tr>
            <td style="height: 20px;"></td>
        </tr>
        <tr>
